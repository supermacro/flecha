{
  "compilerOptions": {
    "strict": true,
    "module": "CommonJS",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "downlevelIteration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./**/*.ts"
  ]
}
